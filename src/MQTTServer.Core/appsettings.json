{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    },
    "File": {
      "LogLevel": {
        "Default": "Warning"
      },
      "Shared": true,
      "Template": "{Timestamp:o} {RequestId,13} [{Level:u3}] {Message} ({EventId:x8}){NewLine}{Exception}",
      "FlushInterval": 2,
      "PathFormat": "Logs/MyApp.log",
      "FileCountLimit": 31,
      "IncludeScopes": true
    },
    "Console": {
      "IncludeScopes": true,
      "LogLevel": {
        "Default": "Information"
      }
    }
  },
  "AllowedHosts": "*",
  "WebServer": {
    "Buffer":  100
  } 
}
