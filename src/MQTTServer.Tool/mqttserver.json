{
  "Logging": {
    "Console": {
      "IncludeScopes": true,
      "LogLevel": {
        "Default": "Information",
        "IMqttLogger": "Warning"
      }
    }
  },
  "Server": {
    "Host": "localhost",
    "Port": 5002,
    "CommunicationTimeout": 15,
    "ConnectionBacklog": 10,
    "MaxPendingMessagesPerClient": 250,
    "Persistent": false,
    "Encrypted": false,
    "CertificationLocation": "",
    "MessageStorageLocation": "./storage.json",
    "ShowSubscriptions": true,
    "ShowMessages": true,
    "ShowClientConnections": true
  },
  "WebServer": {
    "Buffer":  10
  }

}
